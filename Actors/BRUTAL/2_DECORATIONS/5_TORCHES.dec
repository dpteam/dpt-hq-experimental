ACTOR TORCHFLARE
{
	+NOINTERACTION
	+NOGRAVITY
	+CLIENTSIDEONLY
	+FORCEXYBILLBOARD
	RENDERSTYLE ADD
	RADIUS 1
	HEIGHT 1
	ALPHA 0.4
}
ACTOR REDTF : TORCHFLARE
{
	STATES
	{
SPAWN:
SPAWN:
		TNT1 A 0
		TNT1 A 0 A_JUMP(128,2)
		FLAR A 2 BRIGHT
		STOP
		TNT1 A 0
		FLAR B 2 BRIGHT
		STOP
		STOP
	}
}