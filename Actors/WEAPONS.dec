		ASSU A 0 A_PlaySound("m16/cock")
		ASSU MNONMLK 3
		ASSU A 0 A_GiveInventory("Riflecheck",30)
		ASSU A 0 A_TakeInventory("Reload",1)
		Goto Ready
	}
}
